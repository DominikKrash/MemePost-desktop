               <font>
                  <Font size="18.0" />
               </font>
            </Text>
            <Button fx:id="registerButton" layoutX="436.0" layoutY="383.0" mnemonicParsing="false" onMouseClicked="#setRegisterButton" prefHeight="80.0" prefWidth="336.0" text="Register Account" textAlignment="CENTER">
