        </Menu>
        <Menu mnemonicParsing="false" text="Help">
          <items>
            <MenuItem mnemonicParsing="false" text="About MyHelloApp" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
    <AnchorPane maxHeight="1080.0" maxWidth="1920.0" minHeight="720.0" minWidth="1280.0" prefHeight="720.0" prefWidth="1280.0" VBox.vgrow="ALWAYS">
         <children>
            <Text layoutX="511.0" layoutY="115.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Register NOW!!!" textAlignment="CENTER">
               <font>
                  <Font size="36.0" />
               </font>
            </Text>
            <Button fx:id="backButton" layoutX="206.0" layoutY="67.0" mnemonicParsing="false" onMouseClicked="#goBackButton" prefHeight="67.0" prefWidth="103.0" text="HOME" textAlignment="CENTER">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
            <TextField fx:id="loginField" layoutX="620.0" layoutY="179.0" />
            <TextField fx:id="emailField" layoutX="620.0" layoutY="229.0" />
            <TextField fx:id="passwordField" layoutX="620.0" layoutY="278.0" />
            <TextField fx:id="rePasswordField" layoutX="620.0" layoutY="326.0" />
            <Text layoutX="511.0" layoutY="198.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Your login:">
               <font>
                  <Font size="18.0" />
